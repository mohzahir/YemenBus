{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "cr/hashcli": "dev-master",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^6.5|^7.0.1",
        "intervention/image": "^2.5",
        "jeroennoten/laravel-adminlte": "^3.5",
        "laravel/framework": "^6.20",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel_payment/telr": "^1.2",
        "ochui/d7sms": "^1.0",
        "shivella/laravel-bitly": "^1.1",



        "bennett-treptow/laravel-migration-generator": "^4.2",
        "facade/ignition": "^1.16.4",
        "fakerphp/faker": "^1.9.1",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "require-dev": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },"extra": {
        "laravel": {
            "dont-discover": [
                "bennett-treptow/laravel-migration-generator",
                "facade/ignition",
                "fakerphp/faker",
                "laravel/ui",
                "mockery/mockery",
                "nunomaduro/collision",
                "phpunit/phpunit"
            ]
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/Form_helper.php",
            "app/helpers.php"
 ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
         "post-install-cmd": [
            "php artisan clear-compiled",
            "chmod -R 777 public/"
        ]
        
        
    }
}
